import java.util.Scanner;
public class actividad2 {
	
	public static void main(String[] args) {
		
		Scanner scan=new Scanner(System.in);

		System.out.println("----BIENVENIDO USUARIO----");
		System.out.println("");
		System.out.println("Ingresar 2 números y se colocaran todos los enteros en orden");
	
		System.out.println("Ingrese el primer número: ");
			int num1= scan.nextInt();
		
		System.out.println("Ingrese el segundo número: ");
			int num2= scan.nextInt();
			
		System.out.println("");
		System.out.println("Resultado en orden");
		
		 for (int i=num1;i<=num2;i++){ //Los números ingresados por el usuario son acumulados por sus enteros
             System.out.println(i);
		 }
	}	 
}
